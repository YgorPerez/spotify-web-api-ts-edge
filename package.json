{
  "name": "spotify-web-api-ts",
  "version": "1.0.0",
  "description": "An isomorphic TypeScript wrapper for Spotify's Web API",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types",
  "repository": "https://github.com/adamgrieger/spotify-web-api-ts-edge.git",
  "author": "Adam Grieger",
  "license": "MIT",
  "files": [
    "cjs",
    "esm",
    "types",
    "assets"
  ],
  "scripts": {
    "build": "web-scripts build --project tsconfig.build.json",
    "commit": "web-scripts commit",
    "format": "web-scripts format",
    "lint": "web-scripts lint",
    "prepare": "web-scripts audit",
    "release": "web-scripts release",
    "test": "web-scripts test",
    "generate-docs": "typedoc --out docs src/index.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "web-scripts commitmsg",
      "pre-commit": "web-scripts precommit"
    }
  },
  "devDependencies": {
    "@spotify/eslint-config-typescript": "^14.1.6",
    "@spotify/eslint-plugin": "^14.1.6",
    "@spotify/prettier-config": "^14.1.6",
    "@spotify/web-scripts": "^14.1.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "axios": "github:sgammon/axios",
    "qs": "^6.11.1"
  }
}
